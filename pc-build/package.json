{
  "name": "snipe-client",
  "version": "3.1.23",
  "description": "Snipe CR",
  "main": "app/main.js",
  "author": "ArtCS",
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --x64"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.6.1",
    "electron-log": "^5.4.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.snipe.client",
    "productName": "Snipe Client",
    "directories": {
      "output": "dist"
    },
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "python_scripts/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "python_scripts",
        "to": "python_scripts"
      },
      {
        "from": "python-portable",
        "to": "python-portable"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "publisherName": "ArtCS",
      "verifyUpdateCodeSignature": false,
      "sign": null
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "artifactName": "Snipe_Client_Setup_${version}.exe",
      "differentialPackage": false
    },
    "publish": {
      "provider": "generic",
      "url": "http://130.61.118.215:8080/api/app"
    }
  }
}
